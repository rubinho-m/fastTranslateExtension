{
  "name": "Fast translate",
  "description": "Быстрый доступ к сохраненным словам",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "/images/ico16.png",
      "32": "/images/ico32.png",
      "48": "/images/ico48.png",
      "128": "/images/ico128.png"
    },
    "default_popup": "popup.html",
    "default_title": "FastTranslate"
  },
  "icons": {
    "16": "/images/ico16.png",
    "32": "/images/ico32.png",
    "48": "/images/ico48.png",
    "128": "/images/ico128.png"
  }
}
